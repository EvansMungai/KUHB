<div class="form">
  <h3>User Table</h3>
    <table class="table table-striped table-responsive">
        <thead>
          <tr>
              <th scope="col">Username</th>
              <!-- <th scope="col">Hostel Name</th> -->
              <th scope="col">Role</th>
          </tr>
        </thead>
        <tbody>
          <% if (sampleData.length > 0){ %>
              <% sampleData.forEach((data)=>{ %>
                  <tr>
                    <td><%= data.Username %></td>
                    <!-- <td><%= data.Password %></td> -->
                    <td><%= data.Role %></td>
                  </tr>
                <% }) %>
          <% } else { %>
            <tr>
              <td colspan="4">No Data Found</td>
            </tr>
          <% } %>
        </tbody>
      </table>
</div>