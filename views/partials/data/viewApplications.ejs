<div class="form">
  <h3>List of Applications</h3>
  <table class="table table-striped table-responsive">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Period</th>
        <th scope="col">Student</th>
        <th scope="col">Status</th>
        <th scope="col">Hostel</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <% if (sampleData.length> 0){ %>
        <% sampleData.forEach((data)=>{ %>
          <tr>
            <td>
              <%= data.ApplicationNo %>
            </td>
            <td>
              <%= data.ApplicationPeriod %>
            </td>
            <td>
              <%= data.RegistrationNo %>
            </td>
            <td>
              <%= data.Status %>
            </td>
            <td>
              <%= data.PreferredHostel %>
            </td>
            <td>
              <button type="button" class="btn btn-primary btn-sm"><a href="/housekeeper/viewapplications/<%= data.ApplicationNo %>">Review</a></button>
            </td>
          </tr>
          <% }) %>
            <% } else { %>
              <tr>
                <td colspan="4">No Data Found</td>
              </tr>
              <% } %>
    </tbody>
  </table>
</div>