<div class="form">
    <table class="table table-striped table-responsive">
        <thead>
          <tr>
              <th scope="col">Room Number</th>
              <th scope="col">Hostel Number</th>
          </tr>
        </thead>
        <tbody>
          <% if (sampleData.length > 0){ %>
              <% sampleData.forEach((data)=>{ %>
                  <tr>
                    <td><%= data.HostelNo %></td>
                    <td><%= data.HostelName %></td>
                  </tr>
                <% }) %>
          <% } else { %>
            <tr>
              <td colspan="4">No Data Found</td>
            </tr>
          <% } %>
        </tbody>
      </table>
</div>